{
  "expo": {
    "name": "Personal Showcase",
    "slug": "personal-showcase",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "personalshowcase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.aidankeighron.personalshowcase"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/favicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "411d2cce-750a-4d4d-975c-a8819bea7720"
      }
    }
  }
}
